<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ButtonTextProgress">

        <attr name="setTxtConfirm" format="string" />

        <attr name="setTxtTitle" format="string" />

        <attr name="setProgressVisibility" format="enum">

            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
            <enum name="gone" value="2" />

        </attr>

    </declare-styleable>

</resources>